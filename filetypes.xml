    <extensionMap>
      <mapping pattern="ScrollWatcher" type="CoffeeScript" />
      <mapping pattern="ScrollCnt" type="CoffeeScript" />
      <mapping ext="hx" type="HAML" />
      <mapping ext="jp" type="JavaScript" />
      <mapping pattern=".htpasswd" type="PLAIN_TEXT" />
      <mapping pattern="rollover" type="PLAIN_TEXT" />
      <mapping pattern=".bowerrc" type="PLAIN_TEXT" />
      <mapping pattern="tabnavigation" type="PLAIN_TEXT" />
      <mapping pattern=".babelrc" type="PLAIN_TEXT" />
      <mapping ext="dat" type="PLAIN_TEXT" />
      <mapping ext="svg" type="PLAIN_TEXT" />
      <mapping ext="htc" type="PLAIN_TEXT" />
      <mapping pattern="_mixins" type="SCSS" />
      <mapping pattern="cmp_common" type="SCSS" />
    </extensionMap>
  </component>
</application>